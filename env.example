# User and project identification
USER_ID=user123
PROJECT_ID=project456

# Source code configuration
SOURCE_TYPE=local  # Options: local, git, s3
SOURCE_PATH=./source

# Port configuration
EDITOR_PORT=3000
PREVIEW_PORT=5000

# Git configuration (for SOURCE_TYPE=git)
GIT_REPO=https://github.com/username/repo.git
GIT_BRANCH=main
GIT_USERNAME=
GIT_TOKEN=

# S3 configuration (for SOURCE_TYPE=s3)
S3_BUCKET=my-bucket
S3_KEY=path/to/source
AWS_ACCESS_KEY=
AWS_SECRET_KEY=

# Sync configuration
AUTO_SYNC=false
SYNC_INTERVAL=60  # seconds

# Resource limits
CPU_LIMIT=1
MEMORY_LIMIT=1G 